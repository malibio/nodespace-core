{
  "name": "nodespace-app",
  "version": "0.1.0",
  "description": "NodeSpace AI-native knowledge management desktop application",
  "type": "module",
  "scripts": {
    "preinstall": "node ../../enforce-bun.js",
    "dev": "bunx vite dev",
    "build": "bunx vite build",
    "preview": "bunx vite preview",
    "check": "bunx svelte-kit sync && bunx svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bunx svelte-kit sync && bunx svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "bunx oxlint && bunx eslint . --fix",
    "lint:check": "bunx oxlint && bunx eslint .",
    "format": "bunx prettier --write .",
    "format:check": "bunx prettier --check .",
    "quality": "bun run lint:check && bun run format:check && bun run check",
    "quality:fix": "bun run lint && bun run format && bun run check",
    "test": "bunx vitest run",
    "test:watch": "bunx vitest",
    "test:coverage": "bunx vitest run --coverage",
    "analyze": "bunx vite-bundle-analyzer dist",
    "build:analyze": "bun run build && bun run analyze",
    "tauri": "bunx tauri",
    "tauri:dev": "bunx tauri dev -- --bin nodespace-app",
    "tauri:build": "bunx tauri build",
    "dev:server": "cd src-tauri && cargo run --bin dev-server --features dev-server"
  },
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-opener": "^2.5.0",
    "clsx": "^2.1.1",
    "marked": "^16.2.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@internationalized/date": "^3.8.1",
    "@lucide/svelte": "^0.515.0",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.42.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tauri-apps/cli": "^2.8.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/marked": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.21",
    "bits-ui": "^2.8.6",
    "eslint": "^9.32.0",
    "eslint-plugin-filename-rules": "^1.3.1",
    "eslint-plugin-oxlint": "^1.12.0",
    "eslint-plugin-svelte": "^3.11.0",
    "eslint-plugin-unicorn": "^60.0.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^25.0.1",
    "oxlint": "^1.12.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.39.4",
    "svelte-check": "^4.0.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vite-bundle-analyzer": "^0.18.0",
    "vitest": "^2.1.8"
  },
  "engines": {
    "bun": ">=1.0.0",
    "npm": "Please use Bun instead",
    "yarn": "Please use Bun instead",
    "pnpm": "Please use Bun instead"
  },
  "packageManager": "bun@1.0.0"
}
